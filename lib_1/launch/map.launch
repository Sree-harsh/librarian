<?xml version="1.0"?>
<launch>
    <param name="robot_description" textfile="$(find lib_1)/urdf/lib_1.urdf"/>
    
    <!-- <arg name="use_tf_static" default="true"/>  -->
    <arg name="world_file" default="$(find lib_1)/world/empty.world"/>
    <include
    file="$(find gazebo_ros)/launch/empty_world.launch" >
    <arg name="use_sim_time" value="true"/>
		<arg name="verbose" value="false"/>
		<arg name="debug" value="false"/>
		<arg name="gui" value="true" />
		<arg name="world_name" value="$(arg world_file)"/>
    </include>
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
    <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" output="screen"/>
    <node name="tf_map_base_link" pkg="tf" type="static_transform_publisher" args="0 0 0 0 0 0 base_link map 40" />
    <!-- <node name="map_server" pkg="map_server" type="map_server" args="$(find lib_1)/map/mymap.yaml" respawn= "true"/> -->
  
    <node name="gmapping" pkg="gmapping" type="slam_gmapping" output="screen">
        <param name="base_frame" value="base_link"/>
        <param name="odom_frame" value="odom"/>
        <param name="lidar_frame" value="lidar"/>
        <param name="topics_frame" value="/scan"/>
        
    </node>
     <node pkg="move_base" type="move_base" name="Base_move" output="screen">
        <param name="controller_frequency" value="10.0" />
       
        <rosparam file="$(find lib_1)/config/costmap_common_params.yaml" command="load" ns="global_costmap"/>
        <rosparam file="$(find lib_1)/config/costmap_common_params.yaml" command="load" ns="local_costmap"/>
        <rosparam file="$(find lib_1)/config/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find lib_1)/config/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find lib_1)/config/trajectory_planner.yaml" command="load" />
      </node>
    <node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-param robot_description -urdf -model lib_1"
    output="screen" />
    
    <node name="rviz" pkg="rviz" type="rviz" output="screen"/>
    

</launch>